<Ui xmlns="http://www.blizzard.com/wow/ui/" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:schemaLocation="http://www.blizzard.com/wow/ui/ 
 ..\..\FrameXML\UI.xsd">
 
  <Frame name="GSwindow_Cooldown" enableMouse="true" movable="true">
	<Scripts>
  		<OnLoad>
  			self:RegisterForDrag("LeftButton");
  			OnLoad_Message();
  			self.TimeSinceLastUpdate = 0;
  		</OnLoad>
  		<OnUpdate function="GSwindow_OnUpdate" />
  		<OnDragStart>
  			self:StartMoving();
  		</OnDragStart>
  		<OnDragStop>
  			self:StopMovingOrSizing();
  		</OnDragStop>
 	</Scripts>
 <!-- Tags below add a visual element to the frame. -->
 	<Layers>
  		<Layer level="ARTWORK">
   			<Texture setAllPoints="true" file="Interface\AddOns\Baited\background">
   			</Texture>
			<FontString name="$parentSwear" font="Fonts\FRIZQT__.ttf" text="" outline="NORMAL" justifyV="TOP">
  				<Color r="1" g="1" b="1" a="0.5"/>
  				<FontHeight val="17"/>
  				<Anchors>
   					<Anchor point="BOTTOMLEFT" relativeTo="$parent" x="48" y="12" />
  				</Anchors>
			</FontString>
			<FontString name="$parentGCD" font="Fonts\ARIALN.ttf" text="" justifyH="CENTER" justifyV="TOP">
  				<Color r="1" g="1" b="1" a="1.0"/>
  				<FontHeight val="9"/>
  				<Anchors>
   					<Anchor point="BOTTOMLEFT" relativeTo="$parent" x="30" y="5" />
  				</Anchors>
			</FontString>
  		</Layer>
 	</Layers>
 <Size x="128" y="128" />
 <Anchors><Anchor point="CENTER" relativeTo="UIParent"/></Anchors>
</Frame>

  
</Ui> 

